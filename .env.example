# Example environment variables
# Copy this file to .env and fill in your values

# Base Mainnet RPC URL (get from Alchemy, Infura, or QuickNode)
VITE_BASE_MAINNET_RPC=https://mainnet.base.org

# Deployed Contract Address (update after deployment)
VITE_CONTRACT_ADDRESS=0x...

# WalletConnect Project ID (optional, for WalletConnect integration)
VITE_WALLETCONNECT_PROJECT_ID=your_project_id_here

# Application URL (for production builds)
VITE_APP_URL=https://yourdomain.com
